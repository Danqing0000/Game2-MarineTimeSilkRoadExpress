export material bulb_SG_Mat(*)
  = adobe::mtl::standard(
    opacity : float(1.0),
    baseColor : adobe::util::color_texture( texture_2d("deskLamp_STPAOOI/deskLamp_bulb_BaseColor.png", ::tex::gamma_srgb) ),
    translucence : float(0.0),
    glow : float(0.0),
    indexOfRefraction : float(1.6),
    density : float(0.0),
    roughness : adobe::util::float_texture( texture_2d("deskLamp_STPAOOI/deskLamp_bulb_Roughness.png", ::tex::gamma_linear) ),
    normal : adobe::util::normal_texture( texture_2d("deskLamp_STPAOOI/deskLamp_bulb_Normal.png", ::tex::gamma_linear) ),
    interiorColor : color(1.0, 1.0, 1.0),
    metallic : adobe::util::float_texture( texture_2d("deskLamp_STPAOOI/deskLamp_bulb_Metallic.png", ::tex::gamma_linear) ),
    height : float(0.5),
    heightScale : float(1.0)
    );

